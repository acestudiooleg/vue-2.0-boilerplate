{
  "name": "vue-2-boilerplate",
  "version": "0.0.0",
  "description": "A boilerplate for medium to large Vue 2 single page applications",
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "ava",
    "test:watch": "ava -w",
    "gen": "node ./generator/index.js",
    "gcom": "npm run gen -- component",
    "glay": "npm run gen -- layout",
    "gmix": "npm run gen -- mixin",
    "grou": "npm run gen -- route",
    "gser": "npm run gen -- service",
    "gmod": "npm run gen -- module",
    "gtra": "npm run gen -- transformer",
    "gpag": "npm run gen -- page"
  },
  "ava": {
    "files": [
      "src/app/**/*.spec.js"
    ],
    "source": [
      "src/app/**/*.{js,vue}"
    ],
    "match": [],
    "concurrency": 2,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": true,
    "require": [
      "babel-register",
      "./src/test/helpers/setup.js"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petervmeijgaard/vue-2-boilerplate.git"
  },
  "keywords": [
    "Vue",
    "Next",
    "Boilerplate",
    "Webpack",
    "Lint",
    "Medium",
    "Large",
    "SPA",
    "Single",
    "Page",
    "Application"
  ],
  "author": "Peter van Meijgaard <petervanmeijgaard@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/petervmeijgaard/vue-2-boilerplate/issues"
  },
  "homepage": "https://github.com/petervmeijgaard/vue-2-boilerplate#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "proxyquire": "^1.8.0",
    "vue": "^2.2.6",
    "vue-i18n": "^6.1.1",
    "vue-router": "^2.5.1",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-env": "^2.0.31",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "node-localstorage": "^1.3.0",
    "node-sass": "^4.5.3",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "ora": "^1.2.0",
    "pretty": "^2.0.0",
    "proxyquire": "^1.8.0",
    "require-extension-hooks": "^0.2.0",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.3.1",
    "rimraf": "^2.6.0",
    "sass": "^1.0.0-beta.1",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "sinon": "^2.3.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^11.3.4",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserlist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
